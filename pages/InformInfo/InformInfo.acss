/*分割线样式*/

.divLine {
  background: #E0E3DA;
  width: 100%;
  height: 5rpx;
}

.box_txt {
  margin-left: 10px;
}

.box_title {
  font-size: 18px;
  font-weight: bold;
  margin-top: 12px;
}

.stateSucceed {
  margin-top: 3px;
  margin-bottom: 3px;
}

.stateDefeated {
  margin-top: 3px;
  margin-bottom: 3px;
}

.stateSucceed text {
  /* border: 1.5px solid #70bcf6; */
  background-color: #00CCFF;
  font-weight: bold;
  color: #FFFFFF;
  border-radius: 4px;
}

.stateDefeated text {
  /* border: 1.5px solid #70bcf6; */
  background-color: #FF3333;
  font-weight: bold;
  color: #FFFFFF;
  border-radius: 4px;
}

.box_bottom {
  /* display: flex; */
  /* background-color: #09BB07; */
}

.box_bottom button {
  float: right;
  margin-top: -20px;
  margin-right: 10px;
}

.over_box {
  margin: 10px;
  color: #969696;
  text-align: center;
}

.block{
  display: block;
}
.none{
  display: none;
}